
@{
    ViewBag.Title = "DangNhap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="box-register">
    <div class="register-header">
        <header style="color:orange;">Đăng Nhập</header>
    </div>
    <div class="input-box">
        <input type="text" class="input-field" placeholder="Nhập số điện thoại" autocomplete="off" required />
    </div>
    <div class="input-box">
        <input type="password" class="input-field" placeholder="Nhập mật khẩu" autocomplete="off" required />
    </div>

    <div class="input-submit">
        <button class="submit-btn" id="submit"></button>
        <label for="submit">Đăng ký</label>
    </div>
</div>

